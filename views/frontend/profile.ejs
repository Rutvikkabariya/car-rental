                <!-- Main -->
    <div class="main">
        <section class="nav">
            <h2>My Profile</h2>
        </section>
        <div class="form">
              <label for="fname">Full Name</label>
              <input type="text" id="name" name="name" value="<%= user.name%>" placeholder="Enter your name..">
          
              <label for="email">Email Address</label>
              <input type="email" id="email" name="email" value="<%= user.email%>" placeholder="Enter your email..">
          
              <label for="number">Phone Number</label>
              <input type="number" id="number" name="number" value="<%= user.phone%>" placeholder="Enter your number..">
          
              <label for="number">DOB</label>
              <input type="date" id="number" name="dob" value="<%= user.dob %>" placeholder="Enter your dob..">
              
              <label for="address">Address</label>
              <input type="text" id="address" name="address" value="<%= user.address%>" placeholder="Enter your address..">
          
          <!-- 
              <label for="subject">Message</label>
              <textarea id="message" name="message" placeholder="Write your feedback.." style="height:140px"></textarea>
           -->
              <input type="submit" value="Update" onclick="profile()">
        </div>


        

        </main>

