        <section class="places">
            <div class="places-text">
                <h2>Popular Fleets</h2>
                <small>These are some of our popular fleet. But we have others also.</small>
            </div>

            <div class="cards">
            <% data.forEach((car)=>{ %>
                <div class="card">
                    <div class="zoom-img">
                        <div class="img-card">
                            <img src="newImg/<%= car.image%>">
                        </div>
                    </div>

                    <div class="text">
                        <span class="rating"></span>
                        <h2><%= car.detail %></h2>
                        <h3>Name : <%= car.car_name%> | Brand : <%= car.car_brand%> </h3>
                        <h3>Model : <%= car.model%></h3>
                        <p class="cost">$<%= car.price%> / Per Day</p>
                        <div class="card-box">
                            <p class="time">üïì <%= car.km%> km/h</p>
                            <p class="location"><a href="/getBooking?id=<%= car._id%>">‚úà Book Now</a></p>
                        </div>
                    </div>
                </div>
                <% }) %>
